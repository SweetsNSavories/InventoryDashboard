{
    "compilerOptions": {
        "target": "ES2015",
        "module": "es2015",
        "moduleResolution": "node",
        "lib": [
            "es2015",
            "dom"
        ],
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true
    },
    "include": [
        "Dashboard/index.ts",
        "Dashboard/Dashboard.tsx",
        "Dashboard/generated/**/*.ts"
    ]
}